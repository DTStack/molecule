{
    "name": "@dtinsight/molecule",
    "version": "2.0.0-alpha.5",
    "description": "A Web IDE UI Framework built with React.js, inspired by VSCode.",
    "module": "./esm/index.js",
    "typings": "./esm/index.d.ts",
    "files": [
        "esm"
    ],
    "scripts": {
        "dev": "./bin/m.cjs dev",
        "build": "./bin/m.cjs build",
