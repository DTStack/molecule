{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/main",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"]
        },
        "test": {
            "dependsOn": ["^build"]
        },
        "check-types": {
            "cache": false,
            "outputs": []
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        }
    }
}
